<mujoco model="kr810">
  <compiler angle="radian" meshdir="assets" autolimits="true" />

  <option integrator="implicitfast" />

  <default>
    <default class="ur5e">
      <material specular="0.5" shininess="0.25" />
      <joint axis="0 1 0" range="-6.28319 6.28319" armature="0.1" />
      <general gaintype="fixed" biastype="affine" ctrlrange="-3 3" gainprm="2000"
        biasprm="0 -2000 -400" forcerange="-200 200" />
      <default class="size3">
        <default class="size3_limited">
          <joint range="-3.1415 3.1415" />
          <general ctrlrange="-3 3" />
        </default>
      </default>
      <default class="size1">
        <general gainprm="500" biasprm="0 -500 -100" forcerange="-28 28" />
      </default>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2" />
      </default>
      <default class="collision">
        <geom type="capsule" group="3" />
        <default class="eef_collision">
          <geom type="cylinder" />
        </default>
      </default>
      <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4" />
    </default>
  </default>

  <asset>
    <material class="ur5e" name="black" rgba="0.133 0.133 0.133 1" />
    <material class="ur5e" name="orange" rgba="0.95 0.330 0.010 1" />
    <material class="ur5e" name="gray" rgba="0.533 0.533 0.133 1" />

    <material class="ur5e" name="jointgray" rgba="0.278 0.278 0.278 1" />
    <material class="ur5e" name="linkgray" rgba="0.82 0.82 0.82 1" />
    <material class="ur5e" name="urblue" rgba="0.49 0.678 0.8 1" />

    <mesh file="a810_Base.obj" />
    <mesh file="a810_Flange.obj" />
    <mesh file="a810_Link2.obj" />
    <mesh file="a810_Link3.obj" />
    <mesh file="a810_Link4.obj" />
    <mesh file="a810_Link5.obj" />
    <mesh file="a810_Link6.obj" />
    <mesh file="a810_Link7.obj" />
    <mesh file="a810_ToolIO.obj" />


  </asset>

  <worldbody>
    <light name="spotlight" mode="targetbodycom" target="link2" pos="0 -1 2" />
    <body name="base" quat="1 0 0 0" childclass="ur5e">
      <inertial mass="0.6" pos="0 0 0" diaginertia="0.003 0.003 0.003"/>
      <geom mesh="a810_Base" material="black" class="visual"/>
      <body name="link2" pos="0 0 0.035" quat="0.707 0 0 -0.707">
        <inertial mass="4.58" pos="0 0 0" diaginertia="0.003 0.003 0.003"/>
        <joint name="joint1" class="size3" axis="0 0 1"/> <!-- base to link2 -->
        <geom mesh="a810_Link2" material="urblue" class="visual" pos = "0 0 0.169"/>
        <geom type="cylinder" size="0.015 0.075" pos="0.05 -0.01 0.275" euler="1.570796326795 0 0" material="orange"/>
        <!-- <geom class="collision" size="0.06 0.06" pos="0 0 -0.04"/> -->
        <body name="link3" pos="0.05 0.0 0.275" quat="1 0 0 0"> 
          <inertial mass="8.393" pos="0 0 0.2125" diaginertia="0.133886 0.133886 0.0151074"/>
          <joint name="joint2" class="size3" axis="0 1 0"/> <!-- link2 to link3 -->
          <geom mesh="a810_Link3" material="black" class="visual" pos="-0.000 -0.085 0.00" quat="0.5 -0.5 0.5 -0.5"/> 
          <!-- <geom class="collision" pos="0 -0.04 0" quat="1 1 0 0" size="0.06 0.06"/> -->
          <body name="link4" pos="-0.05 -0.085 0.25">
            <inertial mass="2.275" pos="0 0 0.196" diaginertia="0.0311796 0.0311796 0.004095"/>
            <joint name="joint3" class="size3_limited" axis="0 0 1"/> <!-- link3 to link4 -->
            <geom mesh="a810_Link4" material="urblue" class="visual" pos="-0.049 +0.000 0.339" quat="0.5 0.5 -0.5 -0.5"/>
            <!-- <geom class="collision" pos="0 0.08 0" quat="1 1 0 0" size="0.055 0.06"/> -->
            <body name="link5" pos="0.041 0 0.173" quat="1 0 0 0">
              <inertial mass="1.219" pos="0 0.127 0" diaginertia="0.0025599 0.0025599 0.0021942"/>
              <joint name="joint4" class="size1" axis="0 1 0"/>
              <geom mesh="a810_Link5" material="black" class="visual" pos="0.0 0.085 0.000" quat="0.5 -0.5 -0.5 0.5"/>
              <geom type="cylinder" size="0.015 0.075" pos="0 -0.01 0" euler="1.570796326795 0 0" material="orange"/>
              <!-- <geom class="collision" pos="0 0.05 0" quat="1 1 0 0" size="0.04 0.07"/> -->
              <body name="link6" pos="-0.04 0.085 0.331" quat="1 0 0 0">
                <inertial mass="1.219" pos="0 0 0.1" diaginertia="0.0025599 0.0025599 0.0021942"/>
                <joint name="joint5" axis="0 0 1" class="size1"/>
                <geom mesh="a810_Link6" material="urblue" class="visual" pos="0 0.001 0.047" quat="0.7071068 0 0 -0.7071068"/>
                <!-- <geom class="collision" size="0.04 0.06" pos="0 0 0.04"/> -->
                <body name="link7" pos="0 0 0.5" quat="1 0 0 0">
                  <inertial mass="0.1889" pos="0 0.0771683 0" quat="1 0 0 1" diaginertia="0.000132134 9.90863e-05 9.90863e-05"/>
                  <joint name="joint6" class="size1"/>
                  <geom mesh="a810_Link7" material="black" class="visual" quat="1 0 0 0"/>
                  <!-- <geom class="collision" size="0.04 0.06" pos="0 0 0.04"/> -->
                  <body name="toolIO" pos="0 0 0.5">
                  	<inertial mass="0.1889" pos="0 0.0771683 0" quat="1 0 0 1" diaginertia="0.000132134 9.90863e-05 9.90863e-05"/>
                    <joint name="joint7" class="size1"/>
                    <geom mesh="a810_ToolIO" material="black" class="visual"/>
                    <!-- end effector -->
                    <!-- <geom class="eef_collision" pos="0 0.08 0" quat="1 1 0 0" size="0.04 0.02"/> -->
                    <site name="attachment_site" pos="0 0.1 0" quat="-1 1 0 0"/>
                    <body name="end_effector" pos="0 0.1 0" quat="1 0 0 0">
                      <geom type="cylinder" name="end_effector" size="0.03 0.05" rgba="0.7 0.2 0.2 1" pos="0 0.05 0" quat="0.7071068 0.7071068 0 0" mass="0" solimp="0.9 0.95 0.001 0.5 2" solref="0.000002 1"/> <!-- size : radius, half_lenght ; pos : relative to body, pos of center of cylinder -->
                      <site name="ft_sensor" pos="0 0 0"/>
                      <site name="tip" pos="0 0.10 0"/>
                      <!-- <body name="tip_body" pos="0 0.10 0" /> for visualization -->
                    </body>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <velocity class="size3" name="joint1" joint="joint1" kv="50000" />
    <velocity class="size3" name="joint2" joint="joint2" kv="50000" />
    <velocity class="size3" name="joint3" joint="joint3" kv="50000" />
    <velocity class="size1" name="joint4" joint="joint4" kv="50000" />
    <velocity class="size1" name="joint5" joint="joint5" kv="50000" />
    <velocity class="size1" name="joint6" joint="joint6" kv="50000" />
    <velocity class="size1" name="joint7" joint="joint7" kv="50000" />
  </actuator>

  <keyframe>
    <key name="home" qpos="0 0 0 0 0 0 0" ctrl="0 0 0 0 0 0 0" />
  </keyframe>

  <!-- <sensor>
    <force name="force_sensor" site="ft_sensor" />
    <torque name="torque_sensor" site="ft_sensor" />
    <accelerometer name="acc_sensor" site="ft_sensor" />
  </sensor> -->


</mujoco>